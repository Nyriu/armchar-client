import { Trait, TraitList } from './trait' ;

export class Spell {

    label: string ;
    score: number ;
    xp: number ;

}

function parseSpell( ob ) : Ability {
   return { label: ob["rdfs:label"],
            score: ob["arm:hasScore"] || 0,
            xp: ob["arm:hasXP"] || 0
   } as Spell ;
}
export function parseSpells( l: any[] ) : Spell[] {
   return l.map( parseSpell ) ;
}
